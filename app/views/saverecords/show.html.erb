<%- model_class = Saverecord -%>
<div class="page-header">
  <h4><%=t '.title', :default => model_class.model_name.human.titleize %></h4>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= t('display_m.saverecord_m.saving_id_m' ) %>:</strong></dt>
  <dd><%= @saverecord.saving_id %></dd>
  <dt><strong><%= t('display_m.saverecord_m.saving_object_m' ) %>:</strong></dt>
  <dd><%= @saverecord.saving.sobject %></dd>
  <dt><strong><%= t('display_m.saverecord_m.saving_sfrom_m' ) %>:</strong></dt>
  <dd><%= @saverecord.saving.sfrom %></dd>
  <dt><strong><%= t('display_m.saverecord_m.srnowdate_m' ) %>:</strong></dt>
  <dd><%= @saverecord.srnowdate %></dd>
  <dt><strong><%= t('display_m.saverecord_m.srnowvalue_m' ) %>:</strong></dt>
  <dd><%= @saverecord.srnowvalue.to_s(:delimited, delimiter: ',') if @saverecord.srnowvalue != nil %></dd>
  <dt><strong><%= t('display_m.saverecord_m.srnownotice_m' ) %>:</strong></dt>
  <dd><%= @saverecord.srnownotice %></dd>
  <dt><strong><%= t('display_m.saverecord_m.srnowdiary_m' ) %>:</strong></dt>
  <dd><%= @saverecord.srnowdiary %></dd>
  <dt><strong><%= t('display_m.saverecord_m.srnowdetail_m' ) %>:</strong></dt>
  <dd><%= @saverecord.srnowdetail %></dd>
  <dt><strong><%= t('display_m.saverecord_m.srnextdate_m' ) %>:</strong></dt>
  <dd><%= @saverecord.srnextdate %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              saverecords_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_saverecord_path(@saverecord), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              saverecord_path(@saverecord),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
